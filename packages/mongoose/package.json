{
  "name": "@foal/mongoose",
  "version": "0.8.2",
  "description": "FoalTS integration of Mongoose",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "generate:api:docs": "ts-docs-gen --entryFile ./src/index.ts --output ../../docs/api/mongoose",
    "test": "mocha --require ts-node/register \"./src/**/*.spec.ts\"",
    "e2e": "mocha --require ts-node/register \"./e2e/**/*.spec.ts\"",
    "dev:e2e": "mocha --require ts-node/register --watch --watch-extensions ts \"./e2e/**/*.spec.ts\"",
    "dev:test": "mocha --require ts-node/register --watch --watch-extensions ts \"./src/**/*.spec.ts\"",
    "build": "rimraf lib && copy-cli \"./src/**.txt.gz\" lib && tsc -p tsconfig-build.json",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "FoalTS",
    "foal",
    "mongoose",
    "ODM",
    "ORM",
    "database",
    "MongoDB"
  ],
  "bugs": {
    "url": "https://github.com/FoalTS/foal/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FoalTS/foal"
  },
  "homepage": "https://github.com/FoalTS",
  "contributors": [
    "Lo√Øc Poullain <loic.poullain@centraliens.net>"
  ],
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "@types/mongoose": "^5.3.12"
  },
  "peerDependencies": {
    "mongoose": "^5.4.9"
  },
  "devDependencies": {
    "@foal/core": "^0.8.2",
    "@types/mocha": "^2.2.43",
    "@types/node": "^10.5.6",
    "copy": "^0.3.2",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.9",
    "rimraf": "^2.6.2",
    "supertest": "^3.4.2",
    "ts-docs-gen": "0.2.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2"
  }
}
